{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "checkJs": false,
    "skipLibCheck": true,
    "types": ["vite/client", "node"],
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src", "vite.config.ts"]
}
